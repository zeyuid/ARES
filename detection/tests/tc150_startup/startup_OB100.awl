ORGANIZATION_BLOCK OB 100
BEGIN
	L		MD 0
	__ASSERT==	__ACCU 1,	0
	L		100
	T		MD 0
END_ORGANIZATION_BLOCK



ORGANIZATION_BLOCK OB 1
BEGIN
	// assert 4-accus
	__FEATURE	0
	__ASSERT==	__ACCU 1,	4

	// check that OB 100 was executed
	L		MD 0
	__ASSERT==	__ACCU 1,	100


	CALL SFC 46 // STOP CPU
END_ORGANIZATION_BLOCK
